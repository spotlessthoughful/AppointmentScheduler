<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="865.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.GenerateReportsController">
    <Label layoutX="14.0" layoutY="14.0" text="Generate Reports" fx:id="generateReportsLabel">
        <font>
            <Font name="System Bold" size="16.0"/>
        </font>
    </Label>
    <Label layoutX="26.0" layoutY="58.0" text="Appointment Types And Number By Month" fx:id="appointmentTypeMonthLabel"/>
    <Button fx:id="generateCustomersReportButton" layoutX="280.0" layoutY="53.0" mnemonicParsing="false"
            onAction="#generateCustomersReportButtonPressed" text="Generate Report"/>
    <TextArea fx:id="customersReportTextArea" layoutX="27.0" layoutY="85.0" prefHeight="200.0" prefWidth="800.0"/>
    <Label layoutX="27.0" layoutY="314.0" text="Contact Schedules" fx:id="contactScheduleLabel"/>
    <Button fx:id="contactSchedulesReportButton" layoutX="280.0" layoutY="309.0" mnemonicParsing="false"
            onAction="#contactSchedulesReportButtonPressed" text="Generate Report"/>
    <TextArea fx:id="contactScheduleTextArea" layoutX="25.0" layoutY="346.0" prefHeight="200.0" prefWidth="800.0"/>
    <Label layoutX="28.0" layoutY="568.0" text="Total Appointments By Year" fx:id="appointmentByYearLabel"/>
    <Button fx:id="totalAppointmentsReportButton" layoutX="280.0" layoutY="563.0" mnemonicParsing="false"
            onAction="#totalAppointmentsReportButtonPressed" text="Generate Report"/>
    <TextArea fx:id="totalAppointmentsTextArea" layoutX="27.0" layoutY="600.0" prefHeight="200.0"
              prefWidth="800.0"/>
    <Button fx:id="backButton" layoutX="758.0" layoutY="817.0" mnemonicParsing="false" onAction="#backButtonPressed"
            text="Back">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</AnchorPane>
