<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="Controller.LoginController">
    <AnchorPane>
        <Label fx:id="loginLabel" layoutX="115.0" layoutY="10.0" text="Login">
            <font>
                <Font size="30.0"/>
            </font>
        </Label>
        <Label fx:id="usernameLabel" layoutX="10.0" layoutY="70.0" text="Username">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label fx:id="passwordLabel" layoutX="10.0" layoutY="115.0" text="Password">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="usernameTextField" layoutX="120.0" layoutY="70.0">
            <font>
                <Font size="14.0"/>
            </font>
        </TextField>
        <TextField fx:id="passwordTextField" layoutX="120.0" layoutY="115.0">
            <font>
                <Font size="14.0"/>
            </font>
        </TextField>
        <Button fx:id="loginButton" layoutX="240.0" layoutY="160.0" mnemonicParsing="false" onAction="#loginButtonPushed"
                text="Login">
            <font>
                <Font size="12.0"/>
            </font>
        </Button>
        <Button fx:id="exitButton" layoutX="10.0" layoutY="160.0" mnemonicParsing="false" onAction="#exitButtonPushed"
                text="Exit">
            <font>
                <Font size="12.0"/>
            </font>
        </Button>
        <Label fx:id="localeLabel" layoutX="10.0" layoutY="205.0">
            <font>
                <Font size="12.0"/>
            </font>
        </Label>
    </AnchorPane>
</AnchorPane>